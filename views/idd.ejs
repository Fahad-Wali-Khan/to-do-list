<%-include ('header'); -%>
    <div class="box" id="heading">
        <h1><%= listTitle %></h1>
    </div>

    <div class="box">
        <% for(var i=0;i<itemh.length;i++){ %>
        <div class="item">
            <input type="checkbox">
            <p><%=itemh[i]%></p>
        </div>    
        <% } %> 
  
    
    <form action="/" method="post">
        <input type="text" name="txt" autocomplete="off" placeholder="New Item">
        <button type="submit" name="list" value="<%=listTitle%>">+</button>
    </form>
    </div>
    <%-include ('footer'); -%>